{
  "name": "seventy-five",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/hapi__joi": "^17.1.0",
    "@types/jest": "^24.9.1",
    "@types/joi": "^14.3.4",
    "@types/node": "^12.12.36",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.6",
    "@types/react-modal": "^3.10.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-modal": "^3.11.2",
    "react-scripts": "3.4.1"
  },
  "scripts": {
    "prestart": "npm run build:styles",
    "start": "react-scripts start",
    "lint": "eslint --ignore-path .gitignore --ext .js,.ts,.tsx .",
    "check-types": "tsc",
    "prettier": "prettier --ignore-path .gitignore \"**/*.+(js|ts|tsx|json)\"",
    "format": "npm run prettier -- --write",
    "check-format": "npm run prettier -- --list-different",
    "validate": "npm-run-all --parallel check-types check-format lint",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "prebuild": "NODE_ENV=production npm run build:styles",
    "build:styles": "postcss src/styles/tailwind.css -o src/styles/styles.css",
    "build": "npm-run-all validate react-scripts build"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.2",
    "@iconify/icons-uil": "^1.0.11",
    "@iconify/react": "^1.1.3",
    "@types/uuid": "^7.0.3",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "autoprefixer": "^9.7.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-react-hooks": "^3.0.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.1.6",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^7.1.0",
    "prettier": "^2.0.4",
    "tailwindcss": "^1.3.1",
    "typescript": "^3.9.2",
    "uuid": "^8.0.0"
  }
}
